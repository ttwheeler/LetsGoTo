{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": "off",
        "react/prop-types": "off",
        "jsx-a11y/control-has-associated-label": "off",
        "react/jsx-filename-extension": "off"
    }
}
